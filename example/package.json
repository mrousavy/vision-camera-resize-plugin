{
  "name": "vision-camera-resize-plugin-example",
  "version": "0.0.1",
  "private": true,
  "scripts": {
    "android": "react-native run-android",
    "ios": "react-native run-ios",
    "start": "react-native start",
    "build:android": "cd android && ./gradlew assembleDebug --no-daemon --console=plain -PreactNativeArchitectures=arm64-v8a",
    "build:ios": "cd ios && xcodebuild -workspace VisionCameraResizePluginExample.xcworkspace -scheme VisionCameraResizePluginExample -configuration Debug -sdk iphonesimulator CC=clang CPLUSPLUS=clang++ LD=clang LDPLUSPLUS=clang++ GCC_OPTIMIZATION_LEVEL=0 GCC_PRECOMPILE_PREFIX_HEADER=YES ASSETCATALOG_COMPILER_OPTIMIZATION=time DEBUG_INFORMATION_FORMAT=dwarf COMPILER_INDEX_STORE_ENABLE=NO"
  },
  "dependencies": {
    "@shopify/react-native-skia": "^1.2.3",
    "react": "^18.2.0",
    "react-native": "^0.73.2",
    "react-native-reanimated": "^3.10.0",
    "react-native-vision-camera": "^4.0.1",
    "react-native-worklets-core": "^1.2.0"
  },
  "devDependencies": {
    "@babel/core": "^7.23.9",
    "@babel/preset-env": "^7.23.9",
    "@babel/runtime": "^7.23.9",
    "@react-native/babel-preset": "0.73.20",
    "@react-native/metro-config": "0.73.3",
    "@react-native/typescript-config": "0.74.0",
    "babel-plugin-module-resolver": "^5.0.0"
  },
  "engines": {
    "node": ">=18"
  }
}
